@model List<StudentApp.Models.Course>

<h2>Thời khóa biểu</h2>

<table class="table">
    <thead>
        <tr>
            <th>Tên khóa học</th>
            <th>Mô tả</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var course in Model)
        {
            <tr>
                <td>@course.Name</td>
                <td>@course.Description</td>
            </tr>
        }
    </tbody>
</table>
